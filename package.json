  "scripts": {
    "build-storybook": "build-storybook",
    "ci:build": "[ \"$CI\" = 'true' ] && yarn microbundle --tsconfig tsconfig.build.json --jsx 'React.createElement' || :",
    "ci:gen-src-docs": "typedoc --plugin typedoc-plugin-markdown --out docs src",
    "ci:test": "jest --config jest.config.js --collectCoverage",
    "dev": "start-storybook --port 33348 --static-dir ./",
    "patch": "yarn version --patch",
    "test": "jest --config jest.config.js",
    "update": "yarn version --minor",
    "upgrade": "yarn version --upgrade",
    "storybook": "start-storybook -p 6006"
  },
